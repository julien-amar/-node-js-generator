{
    "server": {
        "host": "localhost",
        "port": <%= TOOL_PORT %>
    },

    "mongo": {
        "connection_string": "mongodb://<%= DB_HOST %>/<%= DB_NAME %>"
    },

    "crypto": {
        "authentication": {
            "secret": "<%= SECRET_CRYPTO %>"
        },
        "jwt": {
            "cookie": "ecomm-authentication",
            "secret": "<%= SECRET_JWT %>"
        }
    }
}